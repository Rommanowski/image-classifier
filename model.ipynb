    "def show_train(index):\n",
    "    image, label = dataset_train[index]\n",
    "    img_arr = image.detach().cpu().numpy().squeeze()\n",
    "    plt.imshow(img_arr, cmap='gray')\n",
    "    plt.show()"
   ]
  },
  {
   "cell_type": "code",
